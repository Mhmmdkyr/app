        Paginator::useBootstrap();
        $languages = Language::all();
        $output = [];
        $langs = [];
        foreach ($languages as $lang) {
            if ($lang->default) {
                config()->set('app.locale', $lang->slug);
                config()->set('app.active_lang', $lang);
                config()->set('app.default_lang', $lang);
                config()->set('localized-routes.omit_url_prefix_for_locale', $lang->slug);
            }
        }
        foreach ($languages as $lang) {
            $output[] = $lang->slug;
            $langs[] = $lang;
            if ($lang->slug == request()->segment(1)) {
                config()->set('app.locale', $lang->slug);
                config()->set('app.active_lang', $lang);
            }
        }
        $settings = Setting::where('language_id', config('app.active_lang.id'))->OrWhere('language_id', 0)->get()->pluck('value', 'keyword');
        $default_theme = Theme::where('active', 1)->first();
        config()->set('app.languages', $langs);
        config()->set('localized-routes.supported-locales', $output);
        config()->set('settings', $settings);
        config()->set('default_theme', $default_theme);
        config()->set('app.name', config('settings.short_title'));
    }

    public function license_check(){
      $domain = $_SERVER['HTTP_HOST'];
      $domain = str_replace('www.', '', $domain);
      $domain_ex = explode('.', $domain);
      $end = end($domain_ex);
      array_pop($domain_ex);
      $new_domain = $end.implode('', $domain_ex);
      $hash = md5(md5(md5(md5(sha1($new_domain)))));
      $ilk = substr($hash, 0, 8);
      $iki = substr($hash, 8, 8);
      $uc = substr($hash, 16, 8);
      $dort = substr($hash, 24, 8);
      $ilk = str_replace(substr($ilk, 0, 2), 'NT::', $ilk);
      return strtoupper($ilk."-".$iki."-".$uc."-".$dort);
    }
}
